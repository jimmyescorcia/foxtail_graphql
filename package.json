{
  "name": "Foxtail_API",
  "version": "0.0.1",
  "scripts": {
    "start": "NODE_ENV=staging node -r esm src/start.js",
    "prostart": "NODE_ENV=production node -r esm src/start.js",
    "dev": "NODE_ENV=development nodemon -r esm src/start.js",
    "test": "NODE_ENV=development jest",
    "citest": "NODE_ENV=test jest /src",
    "watchtest": "nodemon --exec \"npm test\"",
    "testw": "jest --watch",
    "testc": "jest --coverage",
    "jest single": "npm test chat",
    "cluster": "pm2 start src/start.js -i 0",
    "build": "babel src -d dist --copy-files",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "10.16.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
